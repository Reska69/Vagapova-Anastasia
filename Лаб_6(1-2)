//задание 1
#include <stdio.h>
#include <math.h>
 
int main(void)
{
    float A,B;
    printf("Введите A:");
    scanf ("%f", &A);
    printf("Введите B:");
    scanf ("%f", &B);
 
  A=A+B;
  B=A-B;
  A=A-B;
 
  printf("\nA:%f\nB:%f\n",A,B);
 
    return 0;
}
//задание 2
#include <stdio.h>
 
int main(void)
{
    float A,B,C;
    printf("Введите A:");
    scanf ("%f", &A);
 
    printf("Введите B:");
    scanf ("%f", &B);
 
    printf("Введите C:");
    scanf ("%f", &C);
 
    A=A+B+C;
    B=A-B-C;
    C=A-B-C;
    A=A-B-C;
 
    printf("\nA:%f\nB:%f\nC:%f\n",A,B,C);
 
    return 0;
}
